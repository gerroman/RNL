Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"PowExpand", "::", "usage"}], "=", 
   "\"\<PowExpand[\!\(\*StyleBox[\"expr\",FontSlant->\"Italic\"]\),\!\(\*\
StyleBox[\"cond\",FontSlant->\"Italic\"]\)] expands powers  in \
\!\(\*StyleBox[\"expr\",FontSlant->\"Italic\"]\) assuming \
\!\(\*StyleBox[\"cond\",FontSlant->\"Italic\"]\) holds.\>\""}], ";"}], "\n", 
 RowBox[{
  RowBox[{"PowExpand", "[", 
   RowBox[{"expr_", ",", 
    RowBox[{"cond_", ":", "True"}]}], "]"}], ":=", 
  RowBox[{"expr", "/.", 
   RowBox[{
    RowBox[{"Power", "[", 
     RowBox[{"ex_", ",", "e_"}], "]"}], ":>", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", "fl", "}"}], ",", 
      RowBox[{
       RowBox[{"fl", "=", 
        RowBox[{"Replace", "[", 
         RowBox[{
          RowBox[{"Replace", "[", 
           RowBox[{
            RowBox[{"FactorList", "[", "ex", "]"}], ",", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"n_Integer", ",", "k_Integer"}], "}"}], ":>", 
             RowBox[{"Sequence", "@@", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{
                 RowBox[{"{", 
                  RowBox[{"#1", ",", 
                   RowBox[{"k", "*", "#2"}]}], "}"}], "&"}], "@@@", 
                RowBox[{"FactorInteger", "[", "n", "]"}]}], ")"}]}]}], ",", 
            RowBox[{"{", "1", "}"}]}], "]"}], ",", "\n", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"{", 
               RowBox[{"n_", ",", "k_Integer"}], "}"}], "/;", 
              RowBox[{"TrueQ", "[", 
               RowBox[{"Quiet", "@", 
                RowBox[{"Simplify", "[", 
                 RowBox[{
                  RowBox[{"n", ">", "0"}], ",", "cond"}], "]"}]}], "]"}]}], ":>", 
             RowBox[{"{", 
              RowBox[{"n", ",", "k"}], "}"}]}], ",", 
            RowBox[{
             RowBox[{
              RowBox[{"{", 
               RowBox[{"n_", ",", "k_Integer"}], "}"}], "/;", 
              RowBox[{"TrueQ", "[", 
               RowBox[{"Quiet", "@", 
                RowBox[{"Simplify", "[", 
                 RowBox[{
                  RowBox[{"n", "<", "0"}], ",", "cond"}], "]"}]}], "]"}]}], ":>", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"-", "n"}], ",", "k"}], "}"}]}], ",", 
            RowBox[{"_", ":>", 
             RowBox[{"Sequence", "[", "]"}]}]}], "}"}], ",", 
          RowBox[{"{", "1", "}"}]}], "]"}]}], ";", 
       RowBox[{
        RowBox[{"Times", "@@", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            SuperscriptBox["#", 
             RowBox[{"#2", "*", "e"}]], "&"}], "@@@", "fl"}], ")"}]}], "*", 
        SuperscriptBox[
         RowBox[{"Factor", "[", 
          FractionBox["ex", 
           RowBox[{"Times", "@@", 
            RowBox[{"Power", "@@@", "fl"}]}]], "]"}], "e"]}]}]}], 
